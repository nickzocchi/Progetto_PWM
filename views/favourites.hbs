<div class="row">
  <div class="col-md-12">
    <div class="panel panel-default collapsed">
      <div class="panel-body">
        <table
          class="table table-hover"
          data-page-size="12"
          data-filter="#filter"
        >
          <thead>
            <tr>
              <th>Date Time</th>
              <th>Title</th>
              <th>Preview</th>
              <th>View</th>
            </tr>
          </thead>
          <tbody>

            {{#each paste}}{{/each}}

          </tbody>
          <tfoot>
            <tr>
              <td colspan="5">
                <ul class="pagination pull-right"></ul>
              </td>
            </tr>
          </tfoot>
        </table>

      </div>
    </div>
  </div>
</div>